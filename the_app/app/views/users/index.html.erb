<h1> Listing users </h1>

<table border="1">
  <tr>
    <th> id </th>
    <th> first_name </th>
    <th> last_name </th>
    <th> email </th>
    <th> address1 </th>
    <th> address2 </th>
    <th> city </th>
    <th> date_of_birth </th>
    <th> password </th>
</tr>
<% @users.each do |user| %>
  <tr>
    <td><%= link_to user.id, user_path(user) %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.email %></td>
    <td><%= user.address1 %></td>
    <td><%= user.address2 %></td>
    <td><%= user.city %></td>
    <td><%= user.date_of_birth %></td>
    <td><%= user.password %></td>
  </tr>
  <% end %>
</table>
